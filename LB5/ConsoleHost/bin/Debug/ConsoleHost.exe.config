<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <system.serviceModel>
    <services>
      
      <service name="LB5.WCFSiplex">
        <endpoint address="" binding="basicHttpBinding" contract="LB5.IWCFSiplex" />
        <!--<endpoint address="" binding="netTcpBinding" contract="LB5.IWCFSiplex" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />-->
        
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:57063/Design_Time_Addresses/LB5/WCFSiplex/" />
            <!--<add baseAddress="net.tcp://localhost:57064/Design_Time_Addresses/LB5/WCFSiplex/" />-->
          </baseAddresses>
        </host>
        
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  
</configuration>