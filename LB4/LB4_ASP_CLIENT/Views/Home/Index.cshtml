<!doctype html>
<html>
<head>

    <meta charset=utf-8>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <script>
    $(document).ready(function () {

        $.ajaxSetup({
            xhrFields: {
                withCredentials: true
            }
        });

        $("#btn1").click(function () {

            let x = $("#x").val();
            let y = $("#y").val();

            let serverUrl = "http://localhost:10000/LB4/Simplex.asmx/AddS"

            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: serverUrl,
                data: '{"x":' + x + ', "y":' + y  + '}',
                dataType: "json",
                success: function (data, status) {
                    $("#div1").html(JSON.parse(data.d));
                },
                error: function (request, status, error) { alert("Error"); }
            });
        });

    });
    </script>

</head>
<body>

    <label>Enter x:</label>
    <br />
    <input id="x" type="number" />
    <br />
    <label>Enter y:</label>
    <br />
    <input id="y" type="number" />
    <br /><br />
    <button id="btn1">AddS</button>
    <br /><br />
    <div id="div1">RESULT HERE</div>

</body>
</html>